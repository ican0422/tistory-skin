// 방문자 count
#count(class="w-full rounded-[12px] bg-[#3a3a3a]")
  .center(class="px-[10px] py-[5px]")
  .title(class="m-0 px-[10px] text-white text-[15px] font-bold tracking-wide text-left") 방문자
  .row(class="grid grid-cols-3 gap-x-[12px] mt-[10px] mb-[10px] justify-items-center")
      .item(class="text-center")
        .num(class="text-white text-[15px] font-bold leading-[1.2]") [##_count_total_##]
        .cap(class="mt-[4px] text-[12px] text-[#bfbfbf]") 전체
      .item(class="text-center")
        .num(class="text-white text-[15px] font-bold leading-[1.2]") [##_count_yesterday_##]
        .cap(class="mt-[4px] text-[12px] text-[#bfbfbf]") 어제
      .item(class="text-center")
        .num(class="text-white text-[15px] font-bold leading-[1.2]") [##_count_today_##]
        .cap(class="mt-[4px] text-[12px] text-[#bfbfbf]") 오늘